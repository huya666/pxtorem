.swipers {
  position: relative;
  width: 100%;
  height: 270rpx;
  overflow: hidden;
}
.swiper_item {
  width: 100%;
  height: 266rpx;
  overflow: hidden;
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
}

.search {
  position: relative;
  margin: 0 32rpx 32rpx;
  background-color: #f8f8f8 !important;
  height: 68rpx;
  border-radius: 34rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 13rpx 28rpx;
  box-sizing: border-box;
}

.search_icon {
  width: 36rpx;
  height: 36rpx;
  background-image: url('https://static.zpimg.cn/public/career_sp/icon/search_icon.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
  margin-right: 8rpx;
}
.search_txt {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.4);
}

.canvas {
  width: 100rpx;
  height: 100rpx;
}

.nav_func {
  width: 100%;
  margin-top: 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.nav_func_course,
.nav_func_cert {
  width: 330rpx;
  height: 136rpx;
  font-size: 34rpx;
  background: linear-gradient(157deg, #1082FF 0%, #1478DC 100%);
  border-radius: 16rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: 400;
}

.nav_func_cert {
  background: linear-gradient(180deg, #1CBC86 0%, #00AC71 100%);
  margin-left: 26rpx;
}

.nav_func_course image,
.nav_func_cert image {
  width: 48rpx;
  height: 48rpx;
  border: none;
}

.nav_func_course text,
.nav_func_cert text {
  margin-left: 8rpx;
}

.course_title {
  width: calc(100% - 64rpx);
  margin-top: 48rpx;
  padding: 0 32rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.course_title_text {
  color: #171A1D;
  font-size: 34rpx;
  font-weight: 500;
}
.course_title_more {
  color: #0089FF;
  font-size: 28rpx;
}

.course_t {
  margin-top: 64rpx;
}

.featured_content {
  width: calc(100% - 64rpx);
  margin-top: 32rpx;
  display: flex;
  flex-direction: column;
  margin-left: 32rpx;
}

.featured_list {
  width: calc(100% - 32rpx);
  box-shadow: 0rpx 4rpx 20rpx 0rpx rgba(222, 222, 222, 0.4);
  border-radius: 16px;
  display: flex;
  padding: 16rpx;
  margin-top: 24rpx;
}

.featured_content .featured_list:nth-of-type(1) {
