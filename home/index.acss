  height: 136rpx;
  border-radius: 16rpx;
}

.featured_list_m {
  width: calc(100% - 268rpx);
  margin-left: 16rpx;
  height: 136rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
}

.featured_list_m_t {
